body, html {
    margin: 115px 0px 0px 0px;
    font-family: sans-serif, arial, tahoma;
}

.back {
    top: 0px;
    position: fixed;
    width: 100%;
    height: 225px;
    background-color: #FFF;
    z-index: 2;
}

.wrapper, .wrapper2 {
    position: absolute;
    width: 100%;
}

.menu {
    position: absolute;
    top: 0px;
    margin-left: 650px;
    width: 350px;
    text-align: right;
}

.menu h2 {
    padding: 2px 10px;
}

.content {
    margin: 0px auto;
    width: 1000px;
}

.menu a {
    display: block;
    text-decoration: none;
    color: rgb(58,4,112);
    padding: 2px 10px;
    -moz-border-radius: 12px;
    border-radius: 12px;
    -webkit-border-radius: 12px;
}

.menu a:hover {
    border: 1px solid gray;
    background: #FFF;
    color: black;       
    padding: 1px 9px;
}

.wrapper2 {
    top: 230px;
    z-index: 1;
}

h1.head {
    margin: 0px;
    padding: 125px 0px 10px 0px;
    font-size: 50px;
    color: #D8D195;
    text-shadow: 2px 2px 5px black;
}

h2 {
    color: rgb(58,4,112);
}

.desc {
    padding: 5px 0px;
    margin: 0px;
    border-bottom: 2px solid black;
    color: #D8D195;
} 

.green {
    background-color: rgb(108,172,43);
}